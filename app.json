{
  "expo": {
    "name": "Coffee Delivery",
    "slug": "coffee-delivery-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coffeedeliverymobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.coffeedeliverymobile"
    },
    "plugins": [["react-native-bootsplash", {"assetsDir": "assets/bootsplash"}]]
  }
}
